#include<stdio.h>
#include<string.h>
int main()
{
    char s[10];
    scanf("%s",s);
    char stack[strlen(s)];
    int top=-1;
    for(int i=0;s[i]!='\0';i++)
    {
        if(s[i]=='(' || s[i]=='{' || s[i]=='[')
        {
            stack[++top]=s[i];
        }
        else
        {
            if(top==-1 || 
            (s[i]==')' && stack[top]!='(')|| (s[i]==']' && stack[top]!='[') || 
            (s[i]=='}' && stack[top]!='{'))
            {
                printf("invalid");
                return 0;
            }
            top--;
        }
    }
    if(top==-1)
    printf("valid");
    else
    printf("invalid");
    return 0;
}

