#include<stdio.h>
#include<stdlib.h>
struct node
{
    int d;
    struct node* next;
};
struct node* create()
{
    struct node* head;
    head=(struct node*)malloc(sizeof(struct node));
    head->next=NULL;
    return head;
}
void insert(struct node* head,int n)
{
    struct node* nn;
    nn=(struct node*)malloc(sizeof(struct node));
    nn->d=n;
    nn->next=head->next;
    head->next=nn;
}
void display(struct node* head)
{
    struct node* temp;
    temp=head->next;
    while(temp!=NULL)
    {
        printf("%d",temp->d);
        temp=temp->next;
    }
}
int main()
{
    struct node* head = create();
    for(int i=5;i>0;i--)
    {
        insert(head,i);
    }
    display(head);
    return 0;
}
